{
    "locales": ["en", "de"],
    "catalogs": [{
        "path": "src/locales/{locale}/messages",
        "include": ["src"],
        "exclude": ["**/node_modules/**"]
    }],
    "format": "po",
    "orderBy": "origin",
    "sourceLocale": "en",
    "fallbackLocales": {
        "default": "en"
    },
    "extractBabelOptions": {
        "presets": ["@babel/preset-typescript"]
    }
}
